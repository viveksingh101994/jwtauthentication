{
  "name": "react-jwt-login",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node ./dist/src/server.js",
    "test:tslint": "tslint -t verbose --project tsconfig.json",
    "test:prettier": "prettier --list-different \"**/*.{ts,js,css,scss,md}\"",
