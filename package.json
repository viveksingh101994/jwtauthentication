    "express": "~4.17.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.4",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/node": "^11.13.8",
    "prettier": "^1.17.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.5"
  }
